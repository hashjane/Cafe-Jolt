<p id="notice"><%= notice %></p>

<h1>Trays</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @trays.each do |tray| %>
      <tr>
        <td><%= link_to 'Show', tray %></td>
        <td><%= link_to 'Edit', edit_tray_path(tray) %></td>
        <td><%= link_to 'Destroy', tray, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Tray', new_tray_path %>
